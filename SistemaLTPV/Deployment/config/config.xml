			<menuitem label="Alta Clientes No Propietarios" className="AltaClientesNoPropietarios" 			roles="Clerk,Manager,Administrator,System" method="openPopup"/>
			<menuitem label="Desbloqueo de Inventarios"     className="DesbloqueoInventario" 				roles="Clerk,Manager,Administrator,System" method="openPopup"/>
			<menuitem label="Reservaciones General" 		className="ReservacionesGeneral" 				roles="Clerk,Manager,Administrator,System" method="openPopup"/>
			<menuitem label="Deposito Unidades a Renta" 	className="DepositoUnidadesRenta" 				roles="Clerk,Manager,Administrator,System" method="openPopup"/>
